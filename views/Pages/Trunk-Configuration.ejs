                      <div class="card-header">
                        <strong>Outgoing Settings</strong>
                      </div>
                      <div class="card-body card-block">
                        <form action="/ConfiguracionTrunk/guardar" method="post"  class="form-horizontal">
                          <div class="row form-group">
                            <div class="col col-md-3"><label for="text-input" class=" form-control-label">Trunk name</label></div>
                            <div class="col-12 col-md-9"><input type="text" id="text-input" name="trunkname"  class="form-control"><small class="form-text text-muted">This is a help text</small></div>
                          </div>
                          <div class="row form-group">
                            <div class="col col-md-3"><label for="text-input" class=" form-control-label">Host</label></div>
                            <div class="col-12 col-md-9"><input type="text" id="text-input" name="host"  class="form-control"><small class="help-block form-text">Please enter your email</small></div>
                          </div>
                          <div class="row form-group">
                            <div class="col col-md-3"><label for="password-input" class=" form-control-label">Username</label></div>
                            <div class="col-12 col-md-9"><input type="username" id="text-input" name="usernameT" class="form-control"><small class="help-block form-text">Please enter a complex password</small></div>
                          </div>
                          <div class="row form-group">
                            <div class="col col-md-3"><label for="text-input" class=" form-control-label">Secret</label></div>
                            <div class="col-12 col-md-9"><input type="password" id="password-input" name="secret" class="form-control"></div>
                          </div>
                          <div class="row form-group">
                            <div class="col col-md-3"><label for="text-input" class=" form-control-label">From-User</label></div>
                            <div class="col-12 col-md-9"><input type="text" id="text-input" name="fromuser"   class="form-control"></div>
                          </div>
                          <div class="row form-group">
                            <div class="col col-md-3"><label for="text-input" class=" form-control-label">Port</label></div>
                            <div class="col-12 col-md-9"><input type="text" id="text-input" name="port" class="form-control"></div>
                          </div>
                      </div>
                    </div>
                  </div>

                  <div class="col-lg-6">
                    <div class="card">
                      <div class="card-header">
