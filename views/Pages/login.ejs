<body >

<div class="sufee-login d-flex align-content-center flex-wrap">
    <div class="container">
        <div class="login-content">
            <div class="login-logo">
                <img class="align-content" src="images/logo1.png" alt="">
                <!--<p class="navbar-brand" style="color:#FFFFFF "><Strong>VG</Strong> Gateway</p>-->
            </div>
            <div class="login-form">
                <form action="/inicio" method="get">
                    <div class="form-group">
                        <div class="input-group">
                            <div class="input-group-addon"><i class="fa fa-user"></i></div>
                            <input type="text" id="username" name="username" placeholder="Username" class="form-control">
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="input-group">
                            <div class="input-group-addon"><i class="fa fa-asterisk"></i></div>
                            <input type="password" id="password" name="password" placeholder="Password" class="form-control">
                        </div>
                    </div>
                    <div class="checkbox">
                        <label>
                            <input type="checkbox"> Remember Me
                        </label>
                    </div>
                    <button type="submit" class="btn btn-success btn-flat m-b-30 m-t-30"> <i class="fa fa-sign-in"></i> Sign in</button>
                </form>

            </div>
        </div>
    </div>
</div>

